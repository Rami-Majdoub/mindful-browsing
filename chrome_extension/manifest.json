{
  "manifest_version": 2,

  "name": "Pause",
  "description": "Pause habitual browsing, and press play on your life.",
  "version": "1.0",

  "permissions": [
    "tabs",
    "webNavigation",
    "http://*/",
    "https://*/"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["style.css"],
      "js": ["dotcom.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}